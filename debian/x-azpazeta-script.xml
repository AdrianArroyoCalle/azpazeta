<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-azpazeta-script">
         <comment xml:lang="en">Azpazeta Script</comment>
         <comment xml:lang="es">Script de Azpazeta</comment>
         <!-- Azpazeta hasn't any Magic Number, it's a ZIP with XML
         <magic priority="50">
                <match type="string" value="\x89PNG" offset="0"/>
         </magic> -->
         <glob pattern="*.azps"/>
  </mime-type>
</mime-info>
